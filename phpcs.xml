<?xml version="1.0"?>
<ruleset name="dvncmd">
	<rule ref="./vendor/mediawiki/mediawiki-codesniffer/MediaWiki" />
	<rule ref="MediaWiki.WhiteSpace.SpaceyParenthesis.SingleSpaceAfterOpenParenthesis">
		<severity>0</severity>
	</rule>
	<rule ref="MediaWiki.WhiteSpace.SpaceyParenthesis.SingleSpaceBeforeCloseParenthesis">
		<severity>0</severity>
	</rule>
	<file>.</file>
	<exclude-pattern>vendor/</exclude-pattern>
	<exclude-pattern>home.php</exclude-pattern>
</ruleset>
